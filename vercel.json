{
  "version": 2,
  "buildCommand": "cd web && npm run build",
  "outputDirectory": "web/.next",
  "installCommand": "cd web && npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://fsw-iron-task-api.up.railway.app"
  },
  "functions": {
    "web/app/api/auth/[...nextauth]/route.ts": {
      "maxDuration": 10
    }
  }
}